{section name = construct loop=$construct}
	<div style="height: {$height}px;" dayofweek="{$dayofweek}" day="{$currentDate}" top="{$construct[construct]*2}" class="selectableitem"></div>
	<div style="height: {$height}px;" dayofweek="{$dayofweek}" day="{$currentDate}" top="{$construct[construct]*2+1}" class="selectableitem zebra"></div>
{/section}
